#include <stdio.h>

struct Student {
    int rollNo;
    int m1, m2, m3;
    int total;
};

int main() {
    struct Student s;

    printf("Enter Roll Number: ");
    scanf("%d", &s.rollNo);

    printf("Enter Marks 1: ");
    scanf("%d", &s.m1);

    printf("Enter Marks 2: ");
    scanf("%d", &s.m2);

    printf("Enter Marks 3: ");
    scanf("%d", &s.m3);

    s.total = s.m1 + s.m2 + s.m3;

    printf("\n--- Student Result ---\n");
    printf("Roll No: %d\n", s.rollNo);
    printf("Total Marks: %d\n", s.total);

    return 0;
}