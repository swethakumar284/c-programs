#include <stdio.h>

int main() {
    int n; // Number of students

    printf("Enter the number of students: ");
    scanf("%d", &n);

    int attendance[n]; // Array to store attendance numbers

    // Input attendance numbers
    for(int i = 0; i < n; i++) {
        printf("Enter attendance number for student %d: ", i + 1);
        scanf("%d", &attendance[i]);
    }

    // Print attendance numbers
    printf("\nAttendance numbers of students:\n");
    for(int i = 0; i < n; i++) {
        printf("Student %d: %d\n", i + 1, attendance[i]);
    }

    return 0;
}